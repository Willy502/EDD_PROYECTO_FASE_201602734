<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="barrios.alejandro.udrawingpage.users.controller.RegisterController">
   <children>
      <Label text="Registro" />
      <TextField fx:id="txtUsername" promptText="Nombre de usuario">
         <VBox.margin>
            <Insets left="50.0" right="50.0" top="10.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="txtNombre" promptText="Nombre">
         <VBox.margin>
            <Insets left="50.0" right="50.0" top="10.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="txtDpi" promptText="DPI">
         <VBox.margin>
            <Insets left="50.0" right="50.0" top="10.0" />
         </VBox.margin>
      </TextField>
      <PasswordField fx:id="txtPassword" promptText="Contraseña">
         <VBox.margin>
            <Insets left="50.0" right="50.0" top="10.0" />
         </VBox.margin>
      </PasswordField>
      <PasswordField fx:id="txtConfirmPassword" promptText="Confirmar contraseña">
         <VBox.margin>
            <Insets left="50.0" right="50.0" top="10.0" />
         </VBox.margin>
      </PasswordField>
      <Button mnemonicParsing="false" text="Registrar" textAlignment="CENTER">
         <VBox.margin>
            <Insets left="100.0" right="100.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#backToLogIn" style="-fx-background-color: transparent;" text="Regresar">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
